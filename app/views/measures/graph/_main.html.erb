<form class="form-horizontal">
  <div class="btn-group" role="group">
    <button type="button" ng-click="changeYAxis(key, this)" ng-repeat="(key, ax) in y_axis_options" class="btn btn-default">{{ax}}</button>
  </div>
  <div class="pull-right">
    <input graph-period ng-model="ini_dt" type="text" class="col-sm-2 form-control" placeholder="Filter end date time" style="width:160px">
    <input graph-period ng-model="end_dt" type="text" class="col-sm-2 form-control" placeholder="Filter begin date time" style="width:160px">
    <button type="button" ng-click="filterByDate()"  class="btn btn-info">Ok</button>
  </div>
  <div class="form-group" style="float:left; text-align: center; display: none;">
    <label for="inputPassword3" class="col-sm-6 control-label">Layout</label>
    <div class="col-sm-10">
      <select class="form-control">
        <option>Simple chart</option>
          <option>WIth back data</option>
          <option>3D</option>
          <option>Data Dispersion</option>
      </select>
    </div>
  </div>
</form>



<highchart id="chart1" config="chartConfig" class="span9" ></highchart>
