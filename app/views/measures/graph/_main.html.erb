<form class="form-horizontal">
  <div class="form-group" style="float:left;">
    <label for="inputEmail3" class="control-label" style="float:left;">Y-axis</label>
    <div class="col-sm-10">
      <select class="form-control" ng-model="actualYAxis" ng-change="changeYAxis(actualYAxis)">
          <option ng-repeat="(key, ax) in axis.y.options" value="{{key}}">{{ax.label}}</option>
      </select>
    </div>
  </div>
  <div class="form-group" style="float:left; text-align: center; display: none;">
    <label for="inputPassword3" class="col-sm-6 control-label">Layout</label>
    <div class="col-sm-10">
      <select class="form-control">
        <option>Simple chart</option>
          <option>WIth back data</option>
          <option>3D</option>
          <option>Data Dispersion</option>
      </select>
    </div>
  </div>
</form>



