<%= form_for([@company, @branch], :html => { :class => "form-horizontal", :role => "form" }) do |f| %>
  <% if @branch.errors.any? %>
    <div class="alert alert-danger alert-dismissable" role="alert">
      <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
      <h4><%= pluralize(@branch.errors.count, "error") %> prohibited this branch from being saved:</h4>

      <ul>
      <% @branch.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

